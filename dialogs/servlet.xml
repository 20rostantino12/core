<dialog xmlns="http://wildfly.org/undertow/servlet" id="servlet-container">
	<container id="servletContainer" label="servletContainer" operator="Concurrency">
		<dmr xmlns="htt://whildfly.org" address="/{selected.profile}/subsystem=undertow/servlet-container=default/setting=jsp"/>
		<container id="details" label="details" operator="Choice">
			<form id="servletContainer#attributes" label="Attributes" operator="Concurrency">
				<dmr xmlns="htt://whildfly.org">
					<attribute name="development"/>
					<attribute name="disabled"/>
				</dmr>
			</form>
			<form id="servletContainer#jsp" label="JSP" operator="Concurrency">
				<dmr xmlns="htt://whildfly.org">
					<attribute name="trim-spaces"/>
					<attribute name="recompile-on-fail"/>
					<attribute name="check-interval"/>
					<attribute name="modification-test-interval"/>
					<attribute name="display-source-fragment"/>
					<attribute name="error-on-use-bean-invalid-class-attribute"/>
					<attribute name="java-encoding"/>
					<attribute name="tag-pooling"/>
					<attribute name="generate-strings-as-char-arrays"/>
				</dmr>
			</form>
			<form id="servletContainer#other" label="Other" operator="Concurrency">
				<dmr xmlns="htt://whildfly.org">
					<attribute name="smap"/>
					<attribute name="keep-generated"/>
					<attribute name="scratch-dir"/>
					<attribute name="display-source-fragment"/>
					<attribute name="error-on-use-bean-invalid-class-attribute"/>
					<attribute name="target-vm"/>
					<attribute name="x-powered-by"/>
					<attribute name="dump-smap"/>
					<attribute name="mapped-file"/>
					<attribute name="source-vm"/>
				</dmr>
			</form>
		</container>
	</container>
</dialog>
